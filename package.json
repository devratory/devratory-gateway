{
  "name": "devlab-gateway",
  "version": "0.0.0",
  "description": "Express Gateway Server for Devlab",
  "engines": {
    "node": ">= 14.16 <15"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "~27.0.1",
    "@types/lodash": "^4.14.175",
    "@types/node": "^14.17.19",
    "@typescript-eslint/eslint-plugin": "~4.31.1",
    "@typescript-eslint/parser": "~4.31.1",
    "eslint": "~7.32.0",
    "eslint-config-prettier": "~8.3.0",
