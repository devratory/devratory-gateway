{
  "name": "devlab-gateway",
  "version": "0.0.0",
  "description": "Express Gateway Server for Devlab",
  "engines": {
    "node": ">= 14.16 <15"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "~27.0.1",
    "@types/lodash": "^4.14.175",
    "@types/node": "^14.17.19",
    "@typescript-eslint/eslint-plugin": "~4.31.1",
    "@typescript-eslint/parser": "~4.31.1",
    "dotenv": "^10.0.0",
    "eslint": "~7.32.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-jest": "~24.4.2",
    "jest": "~27.2.0",
    "nodemon": "^2.0.13",
    "prettier": "~2.4.1",
    "rimraf": "~3.0.2",
    "ts-jest": "~27.0.5",
    "ts-node": "^10.2.1",
    "tsc-watch": "4.2.9",
    "tsutils": "~3.21.0",
    "typescript": "~4.4.3"
  },
  "scripts": {
    "start": "nodemon",
    "debug": "nodemon --inspect",
    "clean": "rimraf coverage build tmp",
    "build:watch": "tsc -w -p tsconfig.json",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node -r dotenv/config dist/main.js dotenv_config_path=../.env\"",
    "start:test": "tsc-watch -p tsconfig.build.json --onSuccess \"node -r dotenv/config dist/main.js dotenv_config_path=../.env.test\"",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"src/**/*.ts\" --fix"
  },
  "dependencies": {
    "@nestjs/common": "^8.1.2",
    "@nestjs/core": "^8.1.2",
    "@nestjs/microservices": "^8.1.1",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.4.0",
    "tslib": "~2.3.1"
  },
  "volta": {
    "node": "14.17.5"
  }
}
